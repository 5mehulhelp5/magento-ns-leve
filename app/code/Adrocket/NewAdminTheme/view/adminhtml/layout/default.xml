<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
		<title>Adrocket Magento Ecommerce</title>
		<css src="Adrocket_NewAdminTheme::css/custom2.css"/>
	</head>
	<body>
		<referenceBlock name="logo">
            <arguments>
            	<argument name="show_part" xsi:type="string">logo</argument>
            	<argument name="edition" translate="true" xsi:type="string">Adrocket Magento Ecommerce</argument>
            	<argument name="logo_image_src" xsi:type="string">Adrocket_NewAdminTheme/images/magento-icon.svg</argument>
            </arguments>
        </referenceBlock>
	</body>
</page>